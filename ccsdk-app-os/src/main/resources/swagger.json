          "application/json"
        ],
        "parameters": [
          {
            "name": "deploymentId",
            "in": "path",
            "description": "ID of deployment to query execution status",
            "required": true,
            "type": "string"
          },
          {
            "name": "tenant",
            "in": "query",
            "description": "tenant name associated with the deployment",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
