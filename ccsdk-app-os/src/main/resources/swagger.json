    },
    "/deployments/{deploymentId}/update": {
      "put": {
        "tags": [
          "Deployments"
        ],
        "summary": "Update an existing deployment",
        "description": "Dispatch a request to Deployment handler mS to use Cloudify deployment update operation ",
